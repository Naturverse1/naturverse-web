{
  "name": "naturverse-web",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev:replit": "concurrently -k \"cd web && vite --host 0.0.0.0 --port 3000\" \"npm run dev:api\"",
    "dev:api": "cross-env PORT=5000 NODE_ENV=development tsx server/index.ts",
    "build": "cd web && vite build",
    "preview": "cd web && vite preview --host 0.0.0.0 --port 3000",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.54.0",
    "hono": "^4.3.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.19",
    "wrangler": "^4.29.0"
  }
}
