{
  "version": 2,
  "framework": "vite",
  "buildCommand": "vite build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "functions": {
    "server/index.ts": { "runtime": "vercel/node" }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/server/index.ts" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/dist/index.html" }
  ]
}