{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": null,
  "buildCommand": "rm -rf dist node_modules/.cache && npm ci --production=false && npx vite build --clearScreen=false",
  "outputDirectory": "dist/public",
  "installCommand": "npm ci --production=false",
  "nodeVersion": "20.x",
  "functions": {},
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|webp|woff2?|ttf|eot))$",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}