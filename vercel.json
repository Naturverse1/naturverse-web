{
  "version": 2,
  "framework": null,
  "buildCommand": "vite build",
  "outputDirectory": "dist/public",
  "installCommand": "npm ci",
  "routes": [
    {
      "src": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|ico|webp|woff2?|ttf|eot)$",
      "dest": "/$1.$2",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    { "src": "/.*", "dest": "/index.html" }
  ]
}