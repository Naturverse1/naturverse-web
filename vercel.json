{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": null,
  "buildCommand": "rm -rf dist node_modules/.cache .vercel tailwind.config.* postcss.config.* && npm ci --force && npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm ci --force",
  "nodeVersion": "20.x",
  "functions": {},
  "env": {
    "NODE_ENV": "production",
    "DISABLE_TAILWIND": "true"
  },
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|webp|woff2?|ttf|eot))$",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}