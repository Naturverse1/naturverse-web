{
  "version": 2,
  "name": "naturverse-fresh",
  "framework": null,
  "buildCommand": "rm -rf dist node_modules/.cache && npm ci && npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm ci",
  "nodeVersion": "20.x",
  "functions": {},
  "public": true,
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|webp|woff2?|ttf|eot))$",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}